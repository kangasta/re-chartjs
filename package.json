{
  "name": "re-chartjs",
  "version": "0.1.0",
  "description": "React wrapper for Chart.js charts.",
  "repository": {
    "type": "git",
    "url": "https://github.com/kangasta/re-chartjs.git"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc -p tsconfig.build.json --emitDeclarationOnly",
    "build:js": "babel src -d dist --ignore '**/__tests__/*,demo/,src/setupTests.ts' --extensions \".ts,.tsx\" --source-maps inline",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "pretest": "npm run lint -- --fix",
    "test": "CI=true jest src/",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@types/chart.js": "^2.9.15",
    "chart.js": "^2.9.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.7",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@types/jest": "^24.0.24",
    "@types/node": "^12.12.21",
    "@types/react": "^16.9.17",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "canvas": "^2.6.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-flowtype": "^4.5.2",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "typescript": "^3.7.4"
  },
  "peerDependencies": {
    "@types/react": "^16.9.17",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  }
}
